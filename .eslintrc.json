{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb-base", "prettier", "stylelint"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["@typescript-eslint"],
    "ignorePatterns": [],
    "rules": {
        "import/prefer-default-export": "off",
        "import/no-unresolved": "off"
    }
}
